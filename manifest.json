{
  "vendor": "vtex",
  "name": "my-wishlists",
  "version": "2.1.5",
  "title": "My Wish Lists",
  "description": "Custom app for managing users' wishlists on the My Account Dashboard",
  "builders": {
    "react": "3.x",
    "messages": "1.x",
    "docs": "0.x",
    "store": "0.x",
    "masterdata": "1.x",
    "graphql": "1.x",
    "node": "6.x"
  },
  "dependencies": {
    "vtex.css-handles": "1.x",
    "vtex.product-context": "0.x",
    "vtex.order-manager": "0.x",
    "vtex.render-runtime": "8.x",
    "vtex.store-graphql": "2.x",
    "vtex.my-account": "1.x",
    "vtex.styleguide": "9.x",
    "vtex.my-account-commons": "1.x",
    "vtex.pixel-manager": "1.x",
    "vtex.order-items": "0.x",
    "vtex.store-resources": "0.x",
    "vtex.checkout-resources": "0.x",
    "vtex.store": "2.x",
    "vtex.search-graphql": "0.x",
    "vtex.checkout-graphql": "0.x",
    "vtex.apps-graphql": "3.x",
    "vtex.store-icons": "0.x"
  },
  "peerDependencies": {
    "vtex.quickorder": "3.x"
  },
  "registries": [
    "smartcheckout"
  ],
  "settingsSchema": {
    "title": "Wishlist Columns",
    "type": "object",
    "access": "public",
    "properties": {
      "imageName": {
        "title": "Image Title",
        "type": "string",
        "description": "image column title",
        "default": "Image"
      },
      "image": {
        "title": "image",
        "type": "boolean",
        "description": "active",
        "default": true
      },
      "skuNameTitle": {
        "title": "skuName column title",
        "type": "string",
        "description": "skuName column title",
        "default": "Name"
      },
      "skuName": {
        "title": "skuName",
        "type": "boolean",
        "description": "active",
        "default": true
      },
      "departmentTitle": {
        "title": "department column title",
        "type": "string",
        "description": "department column title",
        "default": "Department"
      },
      "department": {
        "title": "department",
        "type": "boolean",
        "description": "active",
        "default": true
      },
      "skuReferenceCodeTitle": {
        "title": "skuReferenceCode Column title",
        "type": "string",
        "description": "skuReferenceCode",
        "default": "Part #"
      },
      "skuReferenceCode": {
        "title": "skuReferenceCode",
        "type": "boolean",
        "description": "active",
        "default": true
      },
      "quantityTitle": {
        "title": "quantity column title",
        "type": "string",
        "description": "quantity",
        "default": "Qty"
      },
      "quantity": {
        "title": "quantity",
        "type": "boolean",
        "description": "active",
        "default": true
      },
      "unitValueTitle": {
        "title": "unitValue column title",
        "type": "string",
        "description": "unitValue",
        "default": "Unit Value"
      },
      "unitValue": {
        "title": "unitValue",
        "type": "boolean",
        "description": "active",
        "default": true
      },
      "totalValueTitle": {
        "title": "totalValue column title",
        "type": "string",
        "description": "totalValue",
        "default": "Price"
      },
      "totalValue": {
        "title": "totalValue",
        "type": "boolean",
        "description": "active",
        "default": true
      },
      "notes": {
        "title": "notes",
        "type": "boolean",
        "description": "notes",
        "default": true
      },
      "addTitle": {
        "title": "add column title",
        "type": "string",
        "description": "add",
        "default": "Add"
      },
      "add": {
        "title": "add",
        "type": "boolean",
        "description": "active",
        "default": true
      },
      "removeTitle": {
        "title": "remove column title",
        "type": "string",
        "description": "remove",
        "default": "Remove"
      },
      "remove": {
        "title": "remove",
        "type": "boolean",
        "description": "active",
        "default": true
      }
    }
  },
  "policies": [
    {
      "name": "colossus-fire-event"
    },
    {
      "name": "colossus-write-logs"
    },
    {
      "name": "graphql-query"
    },
    {
      "name": "ADMIN_DS"
    },
    {
      "name": "POWER_USER_DS"
    },
    {
      "name": "vbase-read-write"
    },
    {
      "name": "update-app-settings"
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "portal.vtexcommercestable.com.br",
        "path": "/api/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "{{account}}.vtexcommercestable.com.br",
        "path": "/api/dataentities/*"
      }
    },
    {
      "name": "outbound-access",
      "attrs": {
        "host": "api.vtex.com",
        "path": "/*"
      }
    }
  ],
  "$schema": "https://raw.githubusercontent.com/vtex/node-vtex-api/master/gen/manifest.schema"
}
